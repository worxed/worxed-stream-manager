{
  "name": "worxed-stream-manager-backend",
  "version": "1.0.0",
  "description": "Backend server for Worxed Stream Manager - Twitch overlay and alert system",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node scripts/setup-auth.js",
