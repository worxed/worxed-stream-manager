    "dotenv": "^16.4.0",
    "express": "^4.21.0",
    "node-fetch": "^2.7.0",
    "socket.io": "^4.8.0",
    "tmi.js": "^1.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
