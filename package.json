{
  "name": "worxed-stream-manager",
  "version": "1.0.0",
  "description": "Professional Twitch stream overlay and management system",
  "private": true,
  "scripts": {
    "start": "node supervisor.js",
    "dev": "node supervisor.js",
    "dev:admin": "cd backend/admin && pnpm run dev",
    "dev:frontend": "cd frontend && pnpm run dev",
    "build": "pnpm run build:admin && pnpm run build:frontend",
    "build:admin": "cd backend/admin && pnpm run build",
    "build:frontend": "cd frontend && pnpm run build",
    "install:all": "pnpm install",
    "setup": "cd backend && pnpm run setup"
  },
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "ws": "^8.16.0"
  },
  "keywords": [
    "twitch",
    "streaming",
    "overlay",
    "alerts",
    "obs",
    "terminal",
    "cyberpunk"
  ],
  "author": "worxed",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ]
  }
}
